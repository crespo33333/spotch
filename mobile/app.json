{
  "expo": {
    "name": "Spotch",
    "slug": "spotch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.spotch.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCGaeG60-Fe-g9grrH7GJ290Nqq6UFrDMo"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spotch.app",
      "config": {
        "googleMapsApiKey": "AIzaSyCGaeG60-Fe-g9grrH7GJ290Nqq6UFrDMo"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "spotch"
  }
}